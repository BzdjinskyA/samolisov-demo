<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="name.samolisov.serlvet.app.Application">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="name.samolisov.serlvet.app.application"
            name="Servlet Demo">
         <property
               name="appName"
               value="Servlet Demo">
         </property>
      </product>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="name.samolisov.rcp.demo.application"
            name="Servlet Demo">
      </product>
   </extension>

</plugin>
